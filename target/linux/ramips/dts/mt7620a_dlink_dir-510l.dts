#include "mt7620a.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	compatible = "dlink,dir-510l", "ralink,mt7620a-soc";
	model = "D-Link DIR-510L";

	aliases {
		led-boot = &led_status_green;
		led-failsafe = &led_status_red;
		led-running = &led_status_green;
		led-upgrade = &led_status_red;
	};

	chosen {
		bootargs = "console=ttyS1,57600";
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio0 1 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		wps {
			label = "wps";
			gpios = <&gpio0 2 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_status_green: status_green {
			label = "green:status";
			gpios = <&gpio0 14 GPIO_ACTIVE_HIGH>;
		};

		led_status_red: status_red {
			label = "red:status";
			gpios = <&gpio0 11 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio_export {
		compatible = "gpio-export";
		#size-cells = <0>;

		hw_reboot {
			gpio-export,name = "hw_reboot";
			gpio-export,output = <0>;
			gpios = <&gpio0 0 GPIO_ACTIVE_HIGH>;
		};

		usb_power_1_0A {
			gpio-export,name = "usb_power_1_0A";
			gpio-export,output = <0>;
			gpios = <&gpio0 12 GPIO_ACTIVE_LOW>;
		};

		usb_power_0_5A {
			gpio-export,name = "usb_power_0_5A";
			gpio-export,output = <1>;
			gpios = <&gpio0 13 GPIO_ACTIVE_HIGH>;
		};
	};
};

&ethernet {
	mediatek,portmap = "llllw";
	mtd-mac-address = <&config 0xe490>;
	mtd-mac-address-increment = <(1)>;
};

&uart {
	status = "okay";
};

&gpio0 {
	status = "okay";
};

&gpio1 {
	status = "okay";
};

&gpio2 {
	status = "okay";
};

&gpio3 {
	status = "okay";
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <50000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "jboot";
				reg = <0x0 0x10000>;
				read-only;
			};

			partition@10000 {
				label = "recovery";
				reg = <0x10000 0x200000>;
				read-only;
			};

			partition@210000 {
				compatible = "amit,jimage";
				label = "firmware";
				reg = <0x210000 0xde0000>;
			};

			config: partition@ff0000 {
				label = "config";
				reg = <0xff0000 0x10000>;
				read-only;
			};
		};
	};
};

&ehci {
	status = "okay";
};

&ohci {
	status = "okay";
};

&wmac {
	mtd-mac-address = <&config 0xe490>;
	mtd-mac-address-increment = <(1)>;
};

&pcie {
	status = "okay";
};

&pcie0 {
	mt76x0e@0,0 {
		// compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mtd-mac-address = <&config 0xe490>;
		mtd-mac-address-increment = <(2)>;
		mediatek,mtd-eeprom = <&config 0xe05d>;
		mediatek,2ghz = <0>;
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&gsw {
	mediatek,port4 = "ephy";
};

&state_default {
	default {
		groups = "i2c", "uartf", "ephy", "wled", "rgmii1", "spi refclk";
		function = "gpio";
	};
};
